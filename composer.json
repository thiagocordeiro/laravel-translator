{
    "name": "thiagocordeiro/laravel-translator",
    "description": "Search translation keys and insert into json to be translated",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Thiago Cordeiro",
            "email": "thiagoguetten@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": ">=5.4.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "Translator\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Translator\\Tests\\": "tests/"
        },
        "files": [
            "tests/config.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Translator\\TranslatorServiceProvider"
            ]
        }
    }
}
